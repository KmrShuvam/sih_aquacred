{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/dashboard",
            "dest": "/templates/dashboard.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}